<h3>Listando Administradores</h3>

<div class="container">
  <table class="table table-striped  table-hover ">
    <thead class="thead-dark">

    <tr scope="row">
      <th>ID</th>
      <th>Email</th>
      <th></th>
    </tr>
    </thead>
    <tbody >
    <% @admins.each do |admin| %>
      <tr>
        <td><%= admin.id %></td>
        <td><%= admin.email %></td>
        <td style="width: 150px">
          <%= link_to edit_admins_backoffice_admin_path(admin) do %>
             <i class="fa fa-edit"></i>
             <% end %>
          <button type="button" style="width:25px;height:25px" class="btn btn-danger btn-circle">
            <i class="fa fa-minus"></i>
          </button>
        </td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>